{
  "name": "website",
  "version": "0.5.19",
  "author": "huangjien@gmail.com",
  "copyright": "Copyright (c) 2013 Jien Huang",
  "description": "A blog website template",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "cross-env NODE_OPTION='--inspect' next dev -p 8080",
    "build": "next build",
    "start": "next start -p 8080",
    "push": "docker push huangjien/website",
    "make": "docker buildx build --platform linux/amd64 -t huangjien/website .",
    "exe": "docker run --env-file .env.local -ti -d -p 8080:8080 huangjien/website",
    "lint": "next lint",
    "prettier": "prettier --write ."
  },
  "bugs": {
    "url": "https://github.com/huangjien/blog/issues"
  },
  "dependencies": {
    "@nextui-org/react": "^1.0.0-beta.10",
    "ahooks": "^3.7.6",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "github-markdown-css": "^5.2.0",
    "http-proxy": "^1.18.1",
    "i18next": "^22.4.14",
    "next": "13.3.0",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^12.2.0",
    "react-icons": "^4.8.0",
    "react-toastify": "^9.1.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "yarn": ">=1.22.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "babel-loader": "^9.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "husky": "^8.0.3",
    "prettier": "^2.8.7",
    "style-loader": "^3.3.2"
  }
}
